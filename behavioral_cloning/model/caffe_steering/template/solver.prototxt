# The training protocol buffer definition
net: "trainval.prototxt"
test_iter: 24
test_interval: 170
base_lr: 0.001
weight_decay : 0.0005
type : "Adam"
momentum: 0.9
momentum2: 0.999
lr_policy: "step"
gamma: 0.5
#power: 0.5
stepsize: 1700
display: 170
max_iter: 3400
#max_iter: 50000
# snapshot intermediate results
snapshot: 170
snapshot_prefix: "train/steering"
solver_mode: GPU
random_seed: 777
