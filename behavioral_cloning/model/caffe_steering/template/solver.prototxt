# The training protocol buffer definition
net: "trainval.prototxt"
test_iter: 24
test_interval: 42
base_lr: 0.001
weight_decay : 0.0005
type : "Adam"
momentum: 0.9
momentum2: 0.999
lr_policy: "step"
gamma: 0.5
stepsize: 420
display: 42
max_iter: 1000
# snapshot intermediate results
snapshot: 42
snapshot_prefix: "train/steering"
solver_mode: GPU
